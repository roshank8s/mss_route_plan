<templates id="template_address_multiple_markers_gmap" xml:space="preserve">
    <t t-name="web.AddressMultipleMarkersGmap">
        <!-- Flex container with two columns -->
        <div class="o_address_multiple_markers_gmap" style="display: flex; height: 695px;">
            <!-- Left Column: List of sale orders with route headers -->
            <div class="left-col map-left-view">
                <ul t-ref="recordList" class="dl-list-view"></ul>
            </div>
            <!-- Right Column: Google Map -->
            <div class="right-col map-right-view">
                <div t-ref="googleMap" class="map-cls-view"></div>
            </div>
        </div>
    </t>
    <t t-name="mss_route_plan.FieldServiceMapView" owl="1">
        <!-- Flex container with two columns -->
        <div class="o_address_multiple_markers_gmap" style="display: flex; height: 695px;">
            <!-- Left Column: List of field service tasks with route headers -->
            <div class="left-col map-left-view">
                <ul t-ref="recordList" class="dl-list-view"></ul>
            </div>
            <!-- Right Column: Google Map -->
            <div class="right-col map-right-view">
                <div t-ref="googleMap" class="map-cls-view"></div>
            </div>
        </div>
    </t>
    <t t-name="mss_route_plan.UnifiedRouteMapView" owl="1">
        <div class="o_address_multiple_markers_gmap" style="display: flex; height: 695px;">
            <div class="left-col map-left-view">
                <ul t-ref="recordList" class="dl-list-view"></ul>
            </div>
            <div class="right-col map-right-view">
                <div t-ref="googleMap" class="map-cls-view"></div>
            </div>
        </div>
    </t>
</templates>